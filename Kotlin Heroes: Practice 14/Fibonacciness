fun main() {
    repeat(readln().toInt()) {
        val (a1, a2, a4, a5) = readln().split(' ').map { it.toInt() }
        val c1 = a1 + a2
        val c2 = a4 - a2
        val c3 = a5 - a4
        fun count(c: Int) = 
            (if (c == a1 + a2) 1 else 0) +
            (if (a4 == a2 + c) 1 else 0) +
            (if (a5 == c + a4) 1 else 0)
        println(maxOf(count(c1), count(c2), count(c3)))
    }
}
